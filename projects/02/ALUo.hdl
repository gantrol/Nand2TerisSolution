CHIP ALUo {
    IN x,y,f,no,pcarry,pzr;
    OUT out,carry,zr;

    PARTS:
    And(a=x,b=y,out=xay);
    Or(a=x,b=y,out=xoy);
    And(a=f,b=xoy,out=isadd);
    FullAdder(a=isadd, b=xay, c=pcarry, sum=out0, carry=carry);
    Xor(a=out0, b=no, out=out,out=out1);
    Not(in=out1, out=nout);
    And(a=pzr, b=nout, out=zr);

}